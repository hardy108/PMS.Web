@{
    Layout = null;
}
<html style="height: auto; min-height: 100%;">
<head>
    @Html.Partial("_ReadConfigJs")
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>PMS - Login</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/css/bundles.min.css" rel="stylesheet" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="~/js/bundles.min.js"></script>
    <script src="~/Scripts/authv2.js"></script>
    <script src="~/Scripts/utility.js"></script>
    <script src="~/js/plugins/waitme/waitMe.js"></script>
</head>
<body class="hold-transition login-page">
    <div class="login-box">
        <div class="login-logo">
        </div>
        <div class="login-box-body">
            <div style="height:110px;text-align:center">
                <img src="~/img/logo.png" height="70px" width="300px" />
                <p style="color:darkgoldenrod;font-size:medium;font-family:'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;font-weight:bold"> PLANTATION MANAGEMENT SYSTEM</p>
            </div>

            @RenderBody()
        </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->
</body>
</html>


